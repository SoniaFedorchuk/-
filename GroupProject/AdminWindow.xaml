<Window x:Class="GroupProject.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GroupProject"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid Background="#FF3E3D42">
        <DataGrid x:Name="UsersData" HorizontalAlignment="Left" Height="419" Margin="184,0,0,0" VerticalAlignment="Top" Width="598"/>
        <Button Content="Change role" HorizontalAlignment="Left" Margin="25,20,0,0" VerticalAlignment="Top" Width="137" Click="ChangeRole" Background="#FFBD8E7E" BorderBrush="#FFBD8E7E"/>
        <Button Content="Delete User" HorizontalAlignment="Left" Margin="25,72,0,0" VerticalAlignment="Top" Width="137" Click="DeleteUser" Background="#FFBD8E7E" BorderBrush="#FFBD8E7E"/>

    </Grid>
</Window>
